
<!doctype html>
<html>
<head>
    <title>Challenge You</title>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <style>
        .reading {
             background-image: url(signup.png)  !important;
              background-repeat: no-repeat;
    background-attachment: fixed;
    background-position: left; 
        }
        body         { padding-top:80px; }
    </style>
</head>
<% layout('layout') -%>
<% block('title', page_title) %>
<body class='reading'>
<div class="container text-right">
<div class="col-sm-6 col-sm-offset-3">

        <%= typeof msg != 'undefined' ? msg : '' %>
        

    <h1><span class="fa fa-sign-in"></span> Create Hobby</h1>
    <% for(var i =0; i < hobby.length; i++){ 
        %><div>
            <%= hobby[i].hobby %>
        </div>
   <% }%>

    <!-- LOGIN FORM -->
    <form action="/insert_hobby" method="post">
        <div class="form-group">
            <label>Hobby</label>
            <input type="text" class="form-control" name="hobby">
        </div>
      

        <button type="submit" class="btn btn-warning btn-lg">Create Hobby</button>
    </form>

    <hr>

   =   <p>Or go <a href="/">home</a>.</p>


</div>
</div>
</body>
</html>
           